{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true
      }
    ]
  }
}